{
  "version":2,
  "build":[
    {"src": "*.js","use":"@vercel/node"}
  ],
  "routes":[
    {
      "src": "/(.*)",
      "dest": "/",
      "method": ["GET", "POST", "PUT", "DELETE", "OPTIONS"],
      "allowedHeaders": ["Content-Type", "Authorization"],
    "exposedHeaders": ["Set-Cookie"],
    }
  ]
}
